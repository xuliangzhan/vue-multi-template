{
  "name": "vue-multi-template",
  "version": "1.0.0",
  "description": "A Vue.js multi project template",
  "private": true,
  "scripts": {
    "build": "npm install && npm run build:project1 && npm run build:project2 && npm run build:project3 && npm run build:project4",
    "build:project1": "node build/build.js name=project1",
    "build:project1:zip": "node build/build.js name=project1 zip",
    "build:project2": "node build/build.js name=project2",
    "build:project2:zip": "node build/build.js name=project2 zip",
    "build:project3": "node build/build.js name=project3",
    "build:project3:zip": "node build/build.js name=project3 zip",
    "build:project4": "node build/build.js name=project4",
    "build:project4:zip": "node build/build.js name=project4 zip",
    "clear": "node build/clear.js",
    "dev": "npm run serve",
    "dev:project1": "npm run serve:project1",
    "dev:project2": "npm run serve:project2",
    "dev:project3": "npm run serve:project3",
    "dev:project4": "npm run serve:project4",
    "serve": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "serve:project1": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "serve:project2": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "serve:project3": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "serve:project4": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start:project1": "node build/node-server.js name=project1",
    "start:project2": "node build/node-server.js name=project2",
    "start:project3": "node build/node-server.js name=project3",
    "start:project4": "node build/node-server.js name=project4"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "xe-utils": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "acorn": "^6.2.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
